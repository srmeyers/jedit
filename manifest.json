{
  "name": "Jedit Extensions",
  "description" : "JSON Format and Compare Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens JSON Editor"
    }
  }
}